*---------------------------------------*
|  				%icdcompare 		    |
*---------------------------------------*;
*  %icdcompare is a tool to compare diagnoses between groups, e.g. when comparing different definitions for outcomes or in post-hoc analyses;
*  If dataset contains a separate variable that specifices the icdtype, then you can use %icdpick below;
*  The macro creates 3 datasets ;
* 		*First_only 	- diagnosis found in dataset1 only;
*		*Intersect 		- diagnoses found in both datasets;
*		*Second_only 	- diagnoses found in dataset 2 only;

*  Input
*		dataset1 		- name of dataset1;
*		icd1			- name of variable encoding the icd-code in dataset1;
*		dataset2		- name of dataset2;
*		icd2			- name of variable encoding the icd-code in dataset2;
*		icdversion		- optional parameter that can be either "9" or "10", adding descriptive text from Swedish ICD9/10 included in this program;



%macro icdcompare(dataset1,icd1,dataset2,icd2,icdversion) /parmbuff;
%put Syspbuff contains: &syspbuff;
%put Syntax: icdcompare(dataset1,icd1,dataset2,icd2,icdversion);
%if %length(&icdversion)=0 %then %do;
			proc sql;
			create table FIRST_ONLY as
			select distinct 
				icd1
			from &dataset1
			where &icd1 not in 
			(select distinct
			&icd2
			from &dataset2);
			quit;

			proc sql;
			create table INTERSECT as
			select distinct 
				&icd1
			from &dataset1
			where &icd1 in 
			(select distinct
			&icd2
			from &dataset2);
			quit;

			proc sql;
			create table SECOND_ONLY as
			select distinct 
				&icd2
			from &dataset2 
			where &icd2 not in 
			(select distinct
			&icd1
			from &dataset1);
			quit;
%end;
%else %do;

			proc sql;
			create table FIRST_ONLY as
			select distinct 
				a.&icd1,
				b.*	
			from &dataset1 a left join icd&icdversion._ref b
			on a.&icd1=b.kod
			where &icd1 not in 
			(select distinct
			&icd2
			from &dataset2);
			quit;

			proc sql;
			create table INTERSECT as
			select distinct 
				a.&icd1,
				b.*	
			from &dataset1 a left join icd&icdversion._ref b
			on a.&icd1=b.kod
			where &icd1 in 
			(select distinct
			&icd2
			from &dataset2);
			quit;

			proc sql;
			create table SECOND_ONLY as
			select distinct 
				a.&icd2,
				b.*	
			from &dataset2 a left join icd&icdversion._ref b
			on a.&icd2=b.kod
			where &icd2 not in 
			(select distinct
			&icd1
			from &dataset1);
			quit;
%end;
%mend icdcompare;

*---------------------------------------*
|  				%icdpick 			    |
*---------------------------------------*;
*  %icdpick is a tool to quickly split up datasets that have different version of icd displayed in a long-format;
*  The macro creates a dataset for each version of ICD*

*  Input
*		dataset 		- name of dataset;
*		icdtypevar		- the name of the column/variable that encodes the icd version;
*		icdtype			- the specified icd-versions that will be exported;
*		output			- optional parameter that specifies the name of the output datasets;


*  Example
*  %icdpick(dataset,icd,9 10,data)
*  - reads the dataset "dataset" and looks for a variable called "icd", and outputs it to;
*		- data_9 (where icd=9);
*		- data_10 (where icd=10);

%macro icdpick(dataset,icdtypevar,icdtype,output=icdpick) /parmbuff;
%put Syspbuff contains: &syspbuff;
%put Syntax: icdpick(dataset,icdtypevar,icdtype,output=icdpick);
%let word_cnt=%sysfunc(countw(%superq(icdtype)));
%do i = 1 %to &word_cnt;  
%let var&i=%qscan(%superq(icdtype),&i);
%let name= %scan(&output._&&var&i%,1);
%let print = data &name%str(;);
%let print = &print set &dataset%str(;);
%let print = &print	where &icdtypevar = &&var&i%str(;);
%let print = &print run%str(;);
&print
%end;
%mend icdpick;



